    "scripts": {
      "start-with-test-validator": "start-server-and-test 'solana-test-validator --reset --quiet' http://localhost:8899/health start",
      "clean": "npm run clean:program-c && npm run clean:program-rust",
      "build:program": "cargo build-bpf --manifest-path=./Cargo.toml --bpf-out-dir=dist/program",
      "clean:program": "cargo clean --manifest-path=./Cargo.toml && rm -rf ./dist",
      "test:program": "cargo test-bpf --manifest-path=./Cargo.toml",
      "deploy": "solana program deploy --program-id ./dist/program/farm_fund-keypair.json ./dist/program/farm_fund.so"
    }
  }
  